<dsp:page>
	<div class="garden-center-email-signup">
		<div class="section-row">
			<div class="section-content">
				<h3>Garden Center Email</h3>
				<form id="garden-center-email-signup-form" novalidate>
					<div class="field-group">
						<input type="email" id="garden-center-email" name="garden-center-email" data-validation="email required" data-fieldname="Email Address" aria-label="email address" placeholder="Enter email address" maxlength="255" />
					</div>
					<div class="field-group">
						<input type="submit" id="garden-center-email-submit" class="button primary expand" value="Submit" />
					</div>
				</form>
			</div>
		</div>
	</div>
</dsp:page>
