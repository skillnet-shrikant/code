<span class="error">
  <c:if test="${!empty mainFormHandler.formError}">
    <c:if test="${!empty mainFormHandler.formExceptions}">
      <c:forEach var="errorItem" items='${mainFormHandler.formExceptions}' varStatus="loop">
        <c:out value="${errorItem.message}"/><br/>
      </c:forEach>
      <br />
    </c:if>
  </c:if>
</span>
