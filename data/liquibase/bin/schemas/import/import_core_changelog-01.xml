<changeSet author="pventrapragada" id="001_zipcode_data" runAlways="true" labels="zipcode_import">
    <loadUpdateData file="schemas/import/csv/core/001_mff_zipcode.csv" tableName="mff_zipcode_usa" primaryKey="ID">
        <column name="ID" header="ID"/>
        <column name="ZIPCODE" header="ZIPCODE"/>
        <column name="DECOMMISSIONED" header="DECOMMISSIONED"/>
        <column name="CITY" header="CITY"/>
        <column name="STATE" header="STATE"/>
        <column name="COUNTY" header="COUNTY"/>
        <column name="TIMEZONE" header="TIMEZONE"/>
    </loadUpdateData>
</changeSet>
<changeSet author="cgopalan" id="002_tax_mappings" runAlways="true" labels="tax_mappings">
    <loadUpdateData file="schemas/import/csv/core/002_tax_mappings.csv" tableName="tax_mappings" primaryKey="LEGACY_ID">
        <column name="LEGACY_ID" header="LEGACY_ID"/>
        <column name="CURRENT_ID" header="CURRENT_ID"/>
    </loadUpdateData>
</changeSet>
