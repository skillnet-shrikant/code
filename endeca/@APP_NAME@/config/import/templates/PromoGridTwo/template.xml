<ContentTemplate xmlns="http://endeca.com/schema/content-template/2008"
                 xmlns:editors="editors"
                 xmlns:xavia="http://endeca.com/schema/xavia/2010"
                 type="MainContent">
  <Description>Archived - Use PromoSlotRow Instead</Description>
  <ThumbnailUrl>unavailable.png</ThumbnailUrl>
  <ContentItem>
    <Name>PromoGridTwo</Name>
		<Property name="sectionTitle">
      <String/>
    </Property>
    <Property name="imageURL1">
      <String/>
    </Property>
		<Property name="imageAltText1">
      <String/>
    </Property>
		<Property name="imageH2Text1">
      <String/>
    </Property>
		<Property name="imageParaText1">
      <String/>
    </Property>
		<Property name="overlay1">
			<String/>
		</Property>
		<Property name="overlayVerticalAlignment1">
			<String>top</String>
		</Property>
		<Property name="overlayHorizontalAlignment1">
			<String>left</String>
		</Property>
		<Property name="imageButtonLabel1">
      <String/>
    </Property>
		<Property name="imageButtonHref1">
      <String/>
    </Property>
	<Property name="buttonCustomWidthEnabled1">
        <Boolean>false</Boolean>
    </Property>
	<Property name="buttonCustomWidth1">
		<String/>
	</Property>
	<Property name="noOverLayEnabled1">
        <Boolean>false</Boolean>
    </Property>
		<Property name="imageURL2">
      <String/>
    </Property>
		<Property name="imageAltText2">
      <String/>
    </Property>
		<Property name="imageH2Text2">
      <String/>
    </Property>
		<Property name="imageParaText2">
      <String/>
    </Property>
		<Property name="overlay2">
			<String/>
		</Property>
		<Property name="overlayVerticalAlignment2">
			<String>top</String>
		</Property>
		<Property name="overlayHorizontalAlignment2">
			<String>left</String>
		</Property>
		<Property name="imageButtonLabel2">
      <String/>
    </Property>
		<Property name="imageButtonHref2">
      <String/>
    </Property>
    <Property name="idTag1">
      <String/>
    </Property>
    <Property name="idTag2">
      <String/>
    </Property>
	<Property name="buttonCustomWidthEnabled2">
        <Boolean>false</Boolean>
    </Property>
	<Property name="buttonCustomWidth2">
		<String/>
	</Property>
	<Property name="noOverLayEnabled2">
        <Boolean>false</Boolean>
    </Property>
  </ContentItem>
  <EditorPanel>
    <BasicContentItemEditor>
      <GroupLabel label="PromoGridTwo"/>
	  	<editors:StringEditor propertyName="sectionTitle" label="Section Title" bottomLabel="" enabled="true"/>
		  <GroupLabel label="Promo1"/>
      <editors:StringEditor propertyName="imageURL1" label="Image Path" bottomLabel="${property.media.bottomLabel}" enabled="true"/>
			<editors:StringEditor propertyName="imageAltText1" label="Image Alt Text" bottomLabel="" enabled="true"/>
			<editors:StringEditor propertyName="imageH2Text1" label="H2 Text on Image" bottomLabel="" enabled="true"/>
			<editors:StringEditor propertyName="imageParaText1" label="Para Text on Image" bottomLabel="" enabled="true" height="72"/>
			<editors:BooleanEditor propertyName="noOverLayEnabled1" label="Display Overlay Text below Banner Image" enabled="true" bottomLabel="When Selected Overlay text is displayed below the Image"/>
			<editors:ChoiceEditor propertyName="overlay1" label="${property.overlay.label}" enabled="true">
				<choice label="none (default)" value=""/>
				<choice label="light" value="light"/>
				<choice label="dark" value="dark"/>
			</editors:ChoiceEditor>
			<editors:ChoiceEditor propertyName="overlayVerticalAlignment1" label="${property.overlay.alignment.vertical.label}" enabled="true">
				<choice label="top (default)" value="top"/>
				<choice label="middle" value="middle"/>
				<choice label="bottom" value="bottom"/>
			</editors:ChoiceEditor>
			<editors:ChoiceEditor propertyName="overlayHorizontalAlignment1" label="${property.overlay.alignment.horizontal.label}" enabled="true">
				<choice label="left (default)" value="left"/>
				<choice label="center" value="center"/>
				<choice label="right" value="right"/>
			</editors:ChoiceEditor>
			<editors:StringEditor propertyName="imageButtonLabel1" label="Button Label on Image" bottomLabel="" enabled="true"/>
			<editors:StringEditor propertyName="imageButtonHref1" label="Button Hyperlink on Image" bottomLabel="" enabled="true"/>
			<editors:StringEditor propertyName="idTag1" label="Id Tag" bottomLabel="" enabled="true"/>
			<GroupLabel label="Button1 Custom Width Configuration"/>
			<editors:BooleanEditor propertyName="buttonCustomWidthEnabled1" label="Enable Custom Width For Button" enabled="true"/>
			<editors:NumericStepperEditor propertyName="buttonCustomWidth1" label="Custom Width For Button" bottomLabel="Enter custom Width to be applied for the Button in Percentage" minValue="0" maxValue="100" stepSize="1" enabled="true"/>
	  	<GroupLabel label="Promo2"/>
			<editors:StringEditor propertyName="imageURL2" label="Image Path" bottomLabel="${property.media.bottomLabel}" enabled="true"/>
			<editors:StringEditor propertyName="imageAltText2" label="Image Alt Text" bottomLabel="" enabled="true"/>
			<editors:StringEditor propertyName="imageH2Text2" label="H2 Text on Image" bottomLabel="" enabled="true"/>
			<editors:StringEditor propertyName="imageParaText2" label="Para Text on Image" bottomLabel="" enabled="true" height="72"/>
			<editors:BooleanEditor propertyName="noOverLayEnabled2" label="Display Overlay Text below Banner Image" enabled="true" bottomLabel="When Selected Overlay text is displayed below the Image"/>
			<editors:ChoiceEditor propertyName="overlay2" label="${property.overlay.label}" enabled="true">
				<choice label="none (default)" value=""/>
				<choice label="light" value="light"/>
				<choice label="dark" value="dark"/>
			</editors:ChoiceEditor>
			<editors:ChoiceEditor propertyName="overlayVerticalAlignment2" label="${property.overlay.alignment.vertical.label}" enabled="true">
				<choice label="top (default)" value="top"/>
				<choice label="middle" value="middle"/>
				<choice label="bottom" value="bottom"/>
			</editors:ChoiceEditor>
			<editors:ChoiceEditor propertyName="overlayHorizontalAlignment2" label="${property.overlay.alignment.horizontal.label}" enabled="true">
				<choice label="left (default)" value="left"/>
				<choice label="center" value="center"/>
				<choice label="right" value="right"/>
			</editors:ChoiceEditor>
			<editors:StringEditor propertyName="imageButtonLabel2" label="Button Label on Image" bottomLabel="" enabled="true"/>
			<editors:StringEditor propertyName="imageButtonHref2" label="Button Hyperlink on Image" bottomLabel="" enabled="true"/>
			<editors:StringEditor propertyName="idTag2" label="Id Tag" bottomLabel="" enabled="true"/>
			<GroupLabel label="Button2 Custom Width Configuration"/>
			<editors:BooleanEditor propertyName="buttonCustomWidthEnabled2" label="Enable Custom Width For Button" enabled="true"/>
			<editors:NumericStepperEditor propertyName="buttonCustomWidth2" label="Custom Width For Button" bottomLabel="Enter custom Width to be applied for the Button in Percentage" minValue="0" maxValue="100" stepSize="1" enabled="true"/>
    </BasicContentItemEditor>
  </EditorPanel>
</ContentTemplate>
