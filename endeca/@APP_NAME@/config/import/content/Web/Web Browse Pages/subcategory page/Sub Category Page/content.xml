<ContentItem type="Page" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://endeca.com/schema/content/2008">
  <TemplateId>TwoColumnPage</TemplateId>
  <Name>Two-Column Page</Name>
  <Property name="HeaderContent">
    <ContentItemList type="HeaderContent"/>
  </Property>
  <Property name="SecondaryContent">
    <ContentItemList type="SecondaryContent">
      <ContentItem type="SecondaryContent">
        <TemplateId>Breadcrumbs</TemplateId>
        <Name>Breadcrumbs</Name>
        <Property name="displayNamePropertyAlias">
          <String>displayName</String>
        </Property>
      </ContentItem>
      <ContentItem type="SecondaryContent">
        <TemplateId>ContentSlot-Secondary</TemplateId>
        <Name>Secondary Content Slot</Name>
        <Property name="contentPaths">
          <List xmlns="http://endeca.com/schema/xavia/2010">
            <String>/content/Shared/Guided Navigation</String>
          </List>
        </Property>
        <Property name="templateTypes">
          <List xmlns="http://endeca.com/schema/xavia/2010">
            <String>SecondaryContent</String>
          </List>
        </Property>
        <Property name="templateIds">
          <aaa:List xmlns:aaa="http://endeca.com/schema/xavia/2010"/>
        </Property>
        <Property name="ruleLimit">
          <String>1</String>
        </Property>
      </ContentItem>
    </ContentItemList>
  </Property>
  <Property name="MainContent">
    <ContentItemList type="MainContent">
      <ContentItem type="MainContent">
        <TemplateId>ResultsList</TemplateId>
        <Name>Results List</Name>
        <Property name="boostStrata">
          <aaa:List xmlns:aaa="http://endeca.com/schema/xavia/2010"/>
        </Property>
        <Property name="buryStrata">
          <aaa:List xmlns:aaa="http://endeca.com/schema/xavia/2010"/>
        </Property>
        <Property name="sortOption">
          <aaa:Item class="com.endeca.infront.navigation.model.SortOption" xmlns:aaa="http://endeca.com/schema/xavia/2010">
            <aaa:Property name="label">Default</aaa:Property>
            <aaa:Property name="sorts">
              <aaa:List>
                <aaa:Item class="com.endeca.infront.navigation.model.SortSpec">
                  <aaa:Property name="key">product.description</aaa:Property>
                  <aaa:Property name="descending">false</aaa:Property>
                </aaa:Item>
              </aaa:List>
            </aaa:Property>
          </aaa:Item>
        </Property>
        <Property name="relRank">
          <String>nterms,maxfield,glom,exact,static(sku.margin,descending),static(product.displayName,ascending)</String>
        </Property>
        <Property name="recordsPerPage">
          <String>8</String>
        </Property>
      </ContentItem>
    </ContentItemList>
  </Property>
</ContentItem>